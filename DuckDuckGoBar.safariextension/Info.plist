	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict>
			<key>Start</key>
			<array>
				<string>duckduckgo.js</string>
			</array>
		</dict>
	</dict>
	<key>Description</key>
	<string>Adds DuckDuckGo Extension Bar and Right Click Context Menu Item</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>All</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://vaporstun.com/files/duckduckgo-safari-extension/latest.plist</string>
	<key>Website</key>
	<string>http://github.com/vaporstun/duckduckgo-safari-extension/</string>
